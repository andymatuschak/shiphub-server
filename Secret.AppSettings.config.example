<?xml version="1.0" encoding="utf-8"?>
<!-- These settings override the placeholders in App.config -->
<!-- Add your personal credentials here for development. -->
<appSettings>
  <!-- Get these from https://github.com/organizations/realartists/settings/applications -->
  <!-- You only need the ones you intend to use. -->
  <add key="GitHubCredentials"
       value="appid:secret;appid:secret;..."/>
  <!-- This is a temporary thing used for triggering events while debugging -->
  <!-- It must be issued by one of the above listed applications or you'll get intermittent failures -->
  <add key="GitHubTestToken"
       value ="[token]"/>
</appSettings>
